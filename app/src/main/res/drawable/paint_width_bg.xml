<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <stroke android:color="#ffffffff" android:width="1.0dip"/>
            <solid android:color="#ffffffff"/>
            <corners android:bottomLeftRadius="4.0dip" android:bottomRightRadius="4.0dip" android:topLeftRadius="4.0dip" android:topRightRadius="4.0dip"/>
        </shape>
    </item>
    <item android:state_checked="false">
        <shape>
            <stroke android:color="#ffffffff" android:width="1.0dip"/>
            <solid android:color="#ffff6c6b"/>
            <corners android:bottomLeftRadius="4.0dip" android:bottomRightRadius="4.0dip" android:topLeftRadius="4.0dip" android:topRightRadius="4.0dip"/>
        </shape>
    </item>
</selector>