<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="oval" android:useLevel="false">
            <solid android:color="#ffff6c6b"/>
            <padding android:bottom="1.0dip" android:left="2.0dip" android:right="2.0dip" android:top="1.0dip"/>
            <stroke android:color="@android:color/white" android:width="0.0dip"/>
            <size android:height="12.0dip" android:width="12.0dip"/>
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:shape="oval" android:useLevel="false">
            <solid android:color="#ffffffff"/>
            <padding android:bottom="1.0dip" android:left="2.0dip" android:right="2.0dip" android:top="1.0dip"/>
            <stroke android:color="@android:color/white" android:width="0.0dip"/>
            <size android:height="12.0dip" android:width="12.0dip"/>
        </shape>
    </item>
</selector>